#include <stdlib.h>
#include <stdio.h>

int main()
{
    int t;
    scanf("%d", &t);

    for (int i = 0; i < t; i++)
    {
        int n,temp;
        scanf("%d", &n);
        long  int A[n];
        for (int j = 0; j < n; j++)
        {
            scanf("%ld", &A[j]);
        }
        for(int j=0;j<n-2;j++)
        {
             if(A[i]!=0 && A[i+1]!= 0)
             temp = 0;

             else if(A[i]==0 && A[i+1]== 0)
             temp = 0;
             else
             {
                 if(A[i]==0)
                 temp = A[i+1];
                 else 
                 temp = A[i];
             }
        }
       if(temp== A[n-1])
        printf("YES\n");
        else 
        printf("NO\n");
    }
    return 0;
}
