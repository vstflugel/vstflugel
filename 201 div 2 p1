#include <stdlib.h>
#include <stdio.h>

int main()
{
    int t;
    scanf("%d", &t);

    for (int i = 0; i < t; i++)
    {
        int n, k;
        scanf("%d %d", &n, &k);
        int A[n];
        for (int j = 0; j < n; j++)
        {
            scanf("%d", &A[j]);
        }
        for (int j = 1; j < n+ 1; j++)
        {
            if (j >= n)
            {
                break;
            }
            // if(j==0)
            // {
            //     A[j]++;
            //     A[n-1]--;
            // }
            else
            {
                if (A[j - 1] > 0)
                {
                    A[j]++;
                    A[j - 1]--;
                }
                else
                {
                    j++;
                    A[j]++;
                    A[j - 1]--;
                }
            }
        }
        for (int j = 0; j < n; j++)
        {
            printf("%d ", A[j]);
        }
        printf("\n");
    }
    return 0;
}
